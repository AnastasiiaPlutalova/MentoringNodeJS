{
  "name": "task2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:down:rmi": "docker-compose down --rmi all",
    "start:debug": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:docker": "npm i && nodemon -L -e ts && tsc && node src/server.ts",
    "start:local": "nodemon"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run start:debug",
    "ext": "ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/node": "^12.7.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.0",
    "nodemon": "^1.19.1",
    "standard-version": "^6.0.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/runtime": "^7.5.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.12.2",
    "uuid": "^3.3.2"
  }
}
